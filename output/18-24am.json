      "level": "H3",
      "text": "I_2D = rgb2gray(I_2D);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "end",
      "page": 5
    },
    {
      "level": "H3",
      "text": "I_2D = double(I_2D);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "[m, n] = size(I_2D);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "new_size = min(m, n);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "I_2D = imresize(I_2D, [new_size, new_size]);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "mask = zeros(new_size, new_size);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "mask(1:50, 1:50) = 1;",
      "page": 5
    },
    {
      "level": "H3",
      "text": "C_2D = cov(I_2D);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "[E_2D, D_2D] = eig(C_2D);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "I_2D_KLT = E_2D' * I_2D * E_2D;",
      "page": 5
    },
    {
      "level": "H3",
      "text": "I_2D_KLT_Masked = I_2D_KLT .* mask;",
      "page": 5
    },
    {
      "level": "H3",
      "text": "I_2D_KLT_Reconstructed = E_2D * I_2D_KLT_Masked * E_2D';",
      "page": 5
    },
    {
      "level": "H3",
      "text": "[U, S, V] = svd(I_2D);",
      "page": 5
    },
    {
      "level": "H3",
      "text": "S_Masked = S .* mask;",
      "page": 5
    },
    {
      "level": "H3",
      "text": "I_2D_SVD_Reconstructed = U * S_Masked * V';",
      "page": 5
    },
    {
      "level": "H3",
      "text": "figure;",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,1); imshow(I_2D, []); title('Original Image');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,2); imshow(log(1 + abs(D_2D)), []); title('KLT Spectrum');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,3); imshow(log(1 + abs(S)), []); title('SVD Spectrum');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,4); imshow(I_2D_KLT, []); title('KLT Transformed Image');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,5); imshow(I_2D_SVD_Reconstructed, []); title('SVD Reconstructed Image');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,6); imshow(I_2D_KLT_Reconstructed, []); title('KLT Reconstructed Image');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,7); imshow(abs(I_2D - I_2D_SVD_Reconstructed), []); title('SVD Error');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,8); imshow(abs(I_2D - I_2D_KLT_Reconstructed), []); title('KLT Error');",
      "page": 5
    },
    {
      "level": "H3",
      "text": "subplot(3,3,9); imshow(abs(I_2D_SVD_Reconstructed - I_2D_KLT_Reconstructed), []);",
      "page": 5
    },
